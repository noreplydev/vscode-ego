{
  "name": "ego-lang",
  "displayName": "Ego",
  "description": "Support for the Ego programming language",
  "version": "0.0.1",
  "publisher": "cristian sanchez",
  "engines": {
    "vscode": "^1.89.0"
  },
  "categories": [
    "Programming Languages"
  ],
  "contributes": {
    "languages": [{
      "id": "ego",
      "aliases": ["Ego", "ego"],
