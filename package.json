{
  "name": "ego-lang",
  "displayName": "Ego",
  "description": "Support for the Ego programming language",
  "version": "0.0.2",
  "publisher": "CristianSnchez",
  "engines": {
    "vscode": "^1.89.0"
  },
  "categories": [
    "Programming Languages"
  ],
  "repository": {
    "url": "https://github.com/noreplydev/vscode-ego"
  },
  "icon": "public/logo.png",
  "contributes": {
    "languages": [
      {
        "id": "ego",
        "aliases": [
          "Ego",
          "ego"
        ],
        "extensions": [
          ".ego"
        ],
        "configuration": "./language-configuration.json"
      }
    ],
    "grammars": [
      {
        "language": "ego",
        "scopeName": "source.ego",
        "path": "./syntaxes/ego.tmLanguage.json"
      }
    ]
  }
}